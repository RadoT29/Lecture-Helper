<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<AnchorPane maxHeight="1.7976931348623157E308" prefHeight="116.0" prefWidth="779.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nl.tudelft.oopp.app.controllers.PollCellController">
   <stylesheets>
      <URL value="@styles/svgIcons.css" />
   </stylesheets>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="10.0" />
   </padding>
   <children>
      <VBox prefHeight="200.0" prefWidth="100.0" fx:id="pollBox">
         <children>
            <VBox prefHeight="241.0" prefWidth="496.0">
               <children>
                  <Rectangle arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="67.0" stroke="BLACK" strokeType="INSIDE" width="769.0" />
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Question" />
                  <TextArea fx:id="questionText" prefHeight="93.0" prefWidth="769.0">
                     <VBox.margin>
                        <Insets bottom="20.0" />
                     </VBox.margin></TextArea>
               </children>
            </VBox>
            <VBox fx:id="pollOptionBox" prefHeight="200.0" prefWidth="100.0" />
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button mnemonicParsing="false" onMouseClicked="#addOption" text="Add option" />
                  <Button mnemonicParsing="false" text="Open poll" onMouseClicked="#sendPoll" />
                  <Button mnemonicParsing="false" text="Close poll" onMouseClicked="#closePoll" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
